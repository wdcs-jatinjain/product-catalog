{
  "compilerOptions": {
    "target": "es2018",
    "module": "CommonJS",
     "outDir": "./dist",
    "rootDir": "./src",
    "strict": true,
    "strictPropertyInitialization": false ,
    "esModuleInterop": true,
    "skipLibCheck": true,
     "forceConsistentCasingInFileNames": false,
    "moduleResolution": "node",
    "types": ["node"],
    "resolveJsonModule": true,
    "allowImportingTsExtensions":true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react",
   }
   ,
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}